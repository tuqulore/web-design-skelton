{
  "name": "website-boilerplate",
  "description": "a boilerplate of website provided with npm scripts",
  "version": "2.0.0",
  "author": "tuqulore inc.",
  "bugs": {
    "url": "https://github.com/tuqulore/website-boilerplate/issues"
  },
  "dependencies": {
    "serve-handler": "6.1.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "@11ty/eleventy-img": "1.0.0",
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/typography": "0.5.1",
    "autoprefixer": "10.4.2",
    "cachekill": "3.0.2",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "globby": "11.1.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.6",
    "postcss-cli": "9.1.0",
    "postcss-csso": "5.0.1",
    "postcss-import": "14.0.2",
    "postcss-nested": "5.0.6",
    "prettier": "2.5.1",
    "prettier-plugin-md-nocjsp": "1.2.0",
    "rimraf": "3.0.2",
    "tailwindcss": "2.2.19"
  },
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://github.com/tuqulore/website-boilerplate#readme",
  "keywords": [
    "eleventy",
    "website"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuqulore/website-boilerplate.git"
  },
  "scripts": {
    "build": "NODE_ENV=production run-s clean build:style build:eleventy build:cachekill",
    "build:cachekill": "cachekill -s 'dist/**/!(*.html)' -t 'dist/**/*.{js,css,html}'",
    "build:eleventy": "eleventy",
    "build:style": "postcss 'src/style/**/[^._]*' --dir dist/style --ext css",
    "clean": "rimraf dist",
    "dev": "run-p dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:style": "postcss 'src/style/**/[^._]*' --dir dist/style --ext css --watch",
    "fix": "eslint --fix .",
    "format": "prettier --verbose --write '**/*.{js,json,md,njk,css,scss,pcss,yaml}'",
    "lint": "eslint ."
  }
}
